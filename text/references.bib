  keywords = {Actuators,attitude control,Attitude control,electric actuators,gyro actuator,gyroscopes,PID control,Prototypes,spinning,Spinning,state feedback,State feedback,three-term control,Three-term control,torque control,Torque control,transfer function,transfer functions,Transfer functions,unstructured object,Wheels,Wire},
  month = may,
