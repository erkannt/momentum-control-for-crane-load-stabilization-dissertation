  file = {/Users/hff/Zotero/storage/4VW32ZQQ/Izard et al. - 2017 - Large-scale 3D printing with cable-driven parallel.pdf},
  issn = {2509-8780},
  journal = {Constr Robot},
  keywords = {Additive manufacturing,Cable-driven parallel robots,Innovative construction,On-site digital fabrication,Parametric design,Periodic continuous line},
  month = dec,
  number = {1},
  pages = {69-76},
  title = {Large-Scale {{3D}} Printing with Cable-Driven Parallel Robots},
  volume = {1},
