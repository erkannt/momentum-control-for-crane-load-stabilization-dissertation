  keywords = {Autonomous systems,bicycles,CMG stabilization,Control applications,control moment gyroscope,control system synthesis,dynamic stabilization,Equations,first order sliding mode controller design,gyroscopes,gyroscopic stabilization,high speed flywheel,Mathematical model,nonlinear control systems,nonlinear system dynamics,PID controller,precession torque,proportional-integral-derivative controller,remotely operated vehicles,Sliding mode control,SMC design,stability,tactical steering techniques,three-term control,Torque,two wheeled vehicle,unmanned bicycle,variable structure systems,Variable-structure/sliding-mode control,Vehicle dynamics,velocity control,zero-forward velocity},
  month = jun,
